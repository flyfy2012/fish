@CHARSET "UTF-8";
/*login*/
.fish_group{line-height:40px;height:40px;}
.fish_input{background:none;border:none ;box-shadow:none;color:#fff;font-weight:600;height:35px;line-height:35px;padding:0px 10px;}
.fish_remenber{float:left;color:#fff;}
.fish_check{color:#fff;}
.fish_submit{background:#e54e41;border:0px;padding:8px 40px;}
.fish_ok{border:none;margin:0 auto;width:140px;}
/*index*/
.fish_logo{height:60px;line-height:60px;margin-left:30px;margin-top:5px}
.fish_nav{float:left;height:70px;line-height:70px;margin-top:0px;width:80%;}
.fish_nav_left{height:70px;line-height:70px;width:75%;float:left;}
.fish_time{font-size:20px;border:0px;width:260px;margin:0 auto;display:block;}
.fish_date{font-size:20px;display:block;width:120px;height:70px;line-height:70px;float:left;}
.fish_clock{font-size:20px;width:120px;height:70px;line-height:70px;float:left;padding-left:20px;}
.fish_nav_bar{height:70px;width:25%;float:right;padding-right:70px;padding-top:20px;}
.fish_q{height:35px;width:40px;display:block;padding-right:0px;}
.fish_in{height:70px;line-height:70px;margin:0 auto;}
.fish_bottom{border-bottom:1px solid #fff;width:280px;float:right;}
/*Email*/
.fish_emailIn{width:500px;height:500px;position:absolute;left:50%;top:50%;margin-left:-250px;margin-top:-250px;border:1px solid red;}
/*秒杀*/
.fish_Con{margin-left:30px;margin-top:10px;}
.fish_avat{height:100%;margin:0;line-height:40px;width:150px;float:left;font-weight:700}
.fish_title{height:100%;line-height:40px;float:left;}
.fish_both{height:40px;}



/* goodsview   */

.goods-preview{margin:40px 50px 0;}
.goods-preview p span{font-size:18px;margin-right:60px;}
.goods-preview table th{text-align:center;font-size:18px;}
.goods-preview table{text-align:center;font-size:18px;}


.goods-preview .ele_banner{height:350px;overflow:hidden;position:relative; width:100%;}

.goods-preview .ele_banner .main_image{height:350px;overflow:hidden;position:relative; width:100%;}

.goods-preview.ele_banner .main_image ul{ width:100%;height:350px;overflow:hidden;position:absolute;top:0;left:0;}

.goods-preview .ele_banner .main_image li{float:left;width:100%;height:350px;}

.goods-preview .ele_banner .main_image li img{width:648px;}

.goods-preview .ele_banner div.flicking_con{position:absolute;top:320px;left:0;z-index:999;width:100%;height:10px;margin:0 auto; text-align:center; font-size:0;}
.goods-preview .ele_banner div.flicking_con a{width:10px;height:10px;margin:0px 3px;padding:0; background-color:#000; border-radius:100%;display:inline-block;text-indent:-1000px;}
.goods-preview .ele_banner div.flicking_con a.on{ background-color:#EB1513;}

.goods-preview  h2{font-size:20px;padding:5px 10px;}
.goods-preview  h3{height:30px;background: url('img/chanpin.png')  no-repeat;padding:2px 10px;margin-top:10px;margin-left:10px;}
.goods-preview .product-para{width:620px;text-align:left;margin:0 auto;}
.goods-preview .product-para td{font-size:16px;}
.goods-preview .price{font-size:20px;color:#EB1513;padding:5px 10px;}
.goods-preview  .gou{display:block;width:130px;height:30px;background: url('img/gou.png')  no-repeat;padding:5px 0 0 35px;margin-left:10px;float:left;}
.goods-preview  .song{display:block;width:130px;height:30px;background: url('img/song.png')  no-repeat;padding:5px 0 0 35px;margin-left:10px;float:left;}
/* goodsview end  */


